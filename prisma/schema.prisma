generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  USER_ID        String
  USERNAME       String
  SERVER_ID      String
  LAST_JOINED    Float
  TIME           Float
  STREAMING_TIME Float
  MUTED_TIME     Float

  @@id(name: "USER", [USER_ID, SERVER_ID])
}
