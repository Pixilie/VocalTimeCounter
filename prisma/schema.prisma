generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  discordname   String
  lastjoined    Float
  time          Float
  server        BigInt
  streamingtime Float
  mutedtime     Float

  @@id(name: "user", [discordname, server])
}
